extends layout

block content

  .container
     p(style="background-color:blue; color:white; margin-bottom: 0 !important; ;") 
       | &nbsp; BOS CORONA / EMERALD / VANGUARD <SUPER USER> <CEV.INI>
  .container
     p(style="background-color:white; color:blue; margin-bottom: 0 !important; ;") 
       | &nbsp; System
       | &nbsp; Recipe   
       | &nbsp; Parameter  
       | &nbsp; Constants  
       | &nbsp; Print/Plot
       | &nbsp; Logging  
       | &nbsp; Service  
       | &nbsp; Help
     
  .container(ng-controller='IndexController')
    form.form-horizontal(role="form") 
      .panel.panel-default(style="background-color:gray;")
        .panel-heading
          p(style="background-color:blue; color:white; margin-bottom: 0 !important; ;") Process Control Sequence for Automatic Mode
        .panel-body
          .form-group
            .row
              .col-sm-offset-1.col-sm-2
                label.control-label First Step
              .col-sm-1
                input.form-control(type="text",placeholder="1",style='font-size:14pt;')               
              .col-sm-2
                label.control-label Last Step
              .col-sm-1
                input.form-control(type="text",placeholder="2",style='font-size:14pt;') 
              .col-sm-3
                label.control-label  Step No (Step-mode)
              .col-sm-1
                input.form-control(type="text",placeholder="5",style='font-size:14pt;')
          table.table.borderless
            thead
              tr
                th
                  | State
                th
                  | Step
                th
                  | Process Type
                th
                  | Periods
                th
                  | Set
                th
                  | Description
            tbody
              tr
                td
                  |  
                td
                  | 1		  
                td
                  input.form-control(type="text",placeholder="Etching")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | Etch 60 sec
              tr
                td
                  |  
                td
                  | 2	  
                td
                  input.form-control(type="text",placeholder="Sputtering")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | Gap Deposition, Cath 3
              tr
                td
                  |  
                td
                  | 3  
                td
                  input.form-control(type="text",placeholder="Etching")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="8",size="2")	
                td
                  | 400W S/E, 3 min
              tr
                td
                  |  
                td
                  | 4		  
                td
                  input.form-control(type="text",placeholder="Sputtering")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | Gap Deposition, Cath 3
              tr
                td
                  |  
                td
                  | 5		  
                td
                  input.form-control(type="text",placeholder="No Process")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="6",size="2")	
                td
                  | 
              tr
                td
                  |  
                td
                  | 6		  
                td
                  input.form-control(type="text",placeholder="No Process")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | 
              tr
                td
                  |  
                td
                  | 7		  
                td
                  input#etching.form-control(type="text",placeholder="Etching")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="3",size="2")	
                td
                  | Sputter etch, 60a nife
              tr
                td
                  |  
                td
                  | 8		  
                td
                  input#etching.form-control(type="text",placeholder="Sputtering")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="18",size="2")	
                td
                  | <=200A Gap, 1kW, Cath 3
              tr
                td
                  |  
                td
                  | 9		  
                td
                  input#etching.form-control(type="text",placeholder="No Process")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | 
              tr
                td
                  |  
                td
                  | 10		  
                td
                  input.form-control(type="text",placeholder="No Process")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  input.form-control(type="text",placeholder="1",size="2")	
                td
                  | 
         .form-group
           .container
             .col-sm-offset-1.col-sm-1
               button.btn.btn-default(type="submit") Exit    
             .col-sm-1
               button.btn.btn-default(type="submit") Load 
             .col-sm-1
               button.btn.btn-default(type="submit") Heat 
             .col-sm-1
               button.btn.btn-default(type="submit") Etch 
             .col-sm-2
               button.btn.btn-default(type="submit") Sputter 1 
             .col-sm-2
               button.btn.btn-default(type="submit") Sputter 2 
             .col-sm-2
               button.btn.btn-default(type="submit") Sputter 3 
             .col-sm-1
               button.btn.btn-default(type="submit") Cool 

  .container(ng-controller='GemController')
    .row
      .col-md-6
        .panel.panel-default
          .panel-heading
            | {{ ToolName }} Control
          .panel-body
            form.form-horizontal(role="form")
              .form-group
                label.col-sm-2.control-label Recipe
                .col-sm-10
                  input#inputRecipe.form-control(type="text",placeholder="Recipe",style='font-size:14pt;')               
              .form-group
                .col-sm-offset-2.col-sm-10
                  .radio
                    label LLA
                      input#lla(type="radio",name="optionsRadios",value="A",checked)
                  .radio
                    label LLB
                      input#llb(type="radio",name="optionsRadios",value="B")
                  //.checkbox
                  //  label Engineering Mode
                  //    input(type='checkbox') 
               .form-group
                 .col-sm-offset-2.col-sm-10
                   button.btn.btn-default(type="submit") Start
      .col-md-6
        .panel.panel-default
          .panel-heading
           | Sensors
          .panel-body
            p Current Step number:  {{ Step }}
            p Maximum Step Time: {{ Time }} seconds
            p Chamber Pressure: {{ Pressure }} mTorr
            p RF: {{ RF }} Volt              
     .row
      .col-md-8
        .panel.panel-default
          .panel-heading
            | Sequence
          .panel-body
            p Name: {{ Name }}
            p Helium pressure: {{ Helium }} Torr
            p ESC Chucking Voltage: {{ Voltage }} V
            p Expected Cathode Temp: {{ Temp }} C
      .col-md-4
        .panel.panel-default
          .panel-heading
            | WIP
          .panel-body
            p Lotid: {{ Lotid }}
            p Stage: {{ Stage }} 
            p PPID: {{ PPID }}
            p LoadLock: {{ LoadLock }}

